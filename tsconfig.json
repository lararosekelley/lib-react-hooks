{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "strict": true,
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true
  }
}
